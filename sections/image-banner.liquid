{%- comment -%}
  Preload CSS for faster loading - critical for hero sections
{%- endcomment -%}
<link rel="preload" href="{{ 'section-image-banner.css' | asset_url }}" as="style">
<link rel="stylesheet" href="{{ 'section-image-banner.css' | asset_url }}" media="print" onload="this.media='all'; this.onload=null;">
<noscript>{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}</noscript>

{%- comment -%}
  Critical inline CSS for immediate rendering
{%- endcomment -%}
<style>
  .banner {
    display: flex;
    position: relative;
    flex-direction: column;
    z-index: auto;
    isolation: isolate;
  }
  .banner__media {
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
  }
  .banner__media img,
  .banner__media video {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }
  .banner__content {
    position: relative;
    z-index: 2;
    height: 100%;
  }
  @media screen and (min-width: 750px) {
    .banner--medium:not(.banner--adapt) {
      min-height: 56rem;
    }
  }
</style>

{%- if section.settings.image_height == 'adapt' and section.settings.image != blank -%}
  {%- style -%}
    @media screen and (max-width: 749px) {
      #Banner-{{ section.id }}::before,
      #Banner-{{ section.id }} .banner__media::before,
      #Banner-{{ section.id }}:not(.banner--mobile-bottom) .banner__content::before {
        {%- if section.settings.mobile_image != blank -%}
          padding-bottom: {{ 1 | divided_by: section.settings.mobile_image.aspect_ratio | times: 100 }}%;
        {%- else -%}
          padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
        {%- endif -%}
        content: '';
        display: block;
      }
    }

    @media screen and (min-width: 750px) {
      #Banner-{{ section.id }}::before,
      #Banner-{{ section.id }} .banner__media::before {
        padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;
        content: '';
        display: block;
      }
    }
  {%- endstyle -%}
{%- elsif section.settings.image_height == 'custom' -%}
  {%- style -%}
    #Banner-{{ section.id }} {
      height: {{ section.settings.custom_height_mobile }}vw;
    }
    
    @media screen and (min-width: 750px) {
      #Banner-{{ section.id }} {
        height: {{ section.settings.custom_height_desktop }}vw;
      }
    }
  {%- endstyle -%}
{%- elsif section.settings.image_height == 'fullscreen' -%}
  {%- style -%}
    #Banner-{{ section.id }} {
      height: calc(100svh + 15px);
    }

    @media screen and (min-width: 750px) {
      #Banner-{{ section.id }} {
      height: calc(100vh + 15px);
    }
    }
  {%- endstyle -%}
{%- endif -%}

{%- style -%}
  #Banner-{{ section.id }}::after {
    opacity: {{ section.settings.image_overlay_opacity | divided_by: 100.0 }};
  }
  
  {%- if section.settings.media_blur > 0 -%}
    #Banner-{{ section.id }} .banner__media img,
    #Banner-{{ section.id }} .banner__media video,
    #Banner-{{ section.id }} .banner__media svg {
      filter: blur({{ section.settings.media_blur }}px);
    }
  {%- endif -%}
  
  /* Promo Card Styles */
  .banner__promo-card {
    position: absolute;
    width: 78px;
    border-radius: 8px;
    overflow: hidden;
    z-index: 2;
    text-decoration: none;
  }
  
  @media screen and (max-width: 749px) {
    .banner__promo-card {
      top: 83px;
      right: 20px;
    }
  }
  
  @media screen and (min-width: 750px) {
    .banner__promo-card {
      width: 110px;
      bottom: 40px;
      right: 40px;
    }
  }
  
  .banner__promo-card-image {
    display: block;
    width: 100%;
    height: auto;
    backdrop-filter: blur(12px);
    -webkit-backdrop-filter: blur(12px);
  }
  
  .banner__promo-card-image img {
    width: 100%;
    height: auto;
    display: block;
  }
  
  .banner__promo-card-content {
    background: rgba(255, 255, 255, 0.75);
    padding: 10px;
    text-align: left;
  }
  
  .banner__promo-card-title {
    font-size: 12px;
    font-weight: 600;
    margin: 0;
    color: #000;

  }
  
  .banner__promo-card-subtitle {
    font-size: 12px;
    font-weight: 400;
    margin: 0;
    color: #000;
  }
{%- endstyle -%}

{%- liquid
  assign full_width = '100vw'
  assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'

  if section.settings.image_behavior == 'ambient'
    assign full_width = '120vw'
    assign widths = '450, 660, 900, 1320, 1800, 2136, 2400, 3600, 7680'
  endif
  assign fetch_priority = 'auto'
  if section.index == 1
    assign fetch_priority = 'high'
    assign preload_image = true
  else
    assign preload_image = false
  endif
-%}

{%- comment -%}
  Preload hero media for faster LCP (Largest Contentful Paint)
{%- endcomment -%}
{%- if preload_image -%}
  {%- if section.settings.video != blank -%}
    {%- comment -%} Preload video poster or first frame {%- endcomment -%}
    {%- for source in section.settings.video.sources -%}
      {%- if forloop.first -%}
        <link rel="preload" as="video" href="{{ source.url }}" type="{{ source.mime_type }}">
      {%- endif -%}
    {%- endfor -%}
  {%- elsif section.settings.image != blank -%}
    <link rel="preload" as="image" href="{{ section.settings.image | image_url: width: 1920 }}" media="(min-width: 750px)">
    {%- if section.settings.mobile_image != blank -%}
      <link rel="preload" as="image" href="{{ section.settings.mobile_image | image_url: width: 750 }}" media="(max-width: 749px)">
    {%- else -%}
      <link rel="preload" as="image" href="{{ section.settings.image | image_url: width: 750 }}" media="(max-width: 749px)">
    {%- endif -%}
  {%- endif -%}
{%- endif -%}

<div
  id="Banner-{{ section.id }}"
  class="banner banner--content-align-{{ section.settings.desktop_content_alignment }} banner--content-align-mobile-{{ section.settings.mobile_content_alignment }} banner--{{ section.settings.image_height }}{% if section.settings.image_height == 'adapt' and section.settings.image != blank %} banner--adapt{% endif %}{% if section.settings.show_text_below %} banner--mobile-bottom{%- endif -%}{% if section.settings.show_text_box == false %} banner--desktop-transparent{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}"
>
  {%- if section.settings.video != blank or section.settings.image != blank -%}
    <div class="banner__media media{% if section.settings.image == blank and section.settings.video == blank %} placeholder{% endif %}{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }}{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}">
      {%- if section.settings.video != blank -%}
        <video
          autoplay="true"
          muted="true"
          loop="true"
          playsinline="true"
          webkit-playsinline="true"
          defaultMuted
          preload="auto"
          style="width: 100%; height: 100%; object-fit: cover;"
          id="banner-video-{{ section.id }}"
        >
          {%- for source in section.settings.video.sources -%}
            <source src="{{ source.url }}" type="{{ source.mime_type }}">
          {%- endfor -%}
        </video>
      {%- elsif section.settings.image != blank -%}
        {%- liquid
          assign image_height = section.settings.image.width | divided_by: section.settings.image.aspect_ratio
          assign sizes = full_width
          
          if section.settings.mobile_image != blank
            assign mobile_image_height = section.settings.mobile_image.width | divided_by: section.settings.mobile_image.aspect_ratio
          endif
        -%}
        {%- if section.settings.mobile_image != blank -%}
          <picture>
            <source
              media="(max-width: 749px)"
              srcset="
                {%- if section.settings.mobile_image.width >= 375 -%}{{ section.settings.mobile_image | image_url: width: 375 }} 375w,{%- endif -%}
                {%- if section.settings.mobile_image.width >= 550 -%}{{ section.settings.mobile_image | image_url: width: 550 }} 550w,{%- endif -%}
                {%- if section.settings.mobile_image.width >= 750 -%}{{ section.settings.mobile_image | image_url: width: 750 }} 750w,{%- endif -%}
                {%- if section.settings.mobile_image.width >= 1100 -%}{{ section.settings.mobile_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                {%- if section.settings.mobile_image.width >= 1500 -%}{{ section.settings.mobile_image | image_url: width: 1500 }} 1500w{%- endif -%}
              "
              sizes="100vw"
            >
            {{
              section.settings.image
              | image_url: width: 3840
              | image_tag:
                width: section.settings.image.width,
                height: image_height,
                sizes: sizes,
                widths: widths,
                fetchpriority: fetch_priority
            }}
          </picture>
        {%- else -%}
          {{
            section.settings.image
            | image_url: width: 3840
            | image_tag:
              width: section.settings.image.width,
              height: image_height,
              sizes: sizes,
              widths: widths,
              fetchpriority: fetch_priority
          }}
        {%- endif -%}
      {%- endif -%}
    </div>
  {%- else -%}
    <div class="banner__media media placeholder{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }}{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}">
      {{ 'product-1' | placeholder_svg_tag }}
    </div>
  {%- endif -%}
  <div class="banner__content banner__content--{{ section.settings.desktop_content_position }} page-width{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
    <div class="banner__box content-container content-container--full-width-mobile color-{{ section.settings.color_scheme }} gradient">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'heading' -%}
            <h1
              class="banner__heading inline-richtext {{ block.settings.heading_size }}"
              {{ block.shopify_attributes }}
            >
              {{ block.settings.heading | newline_to_br }}
            </h1>
          {%- when 'text' -%}
            <div class="banner__text rte {{ block.settings.text_style }}" {{ block.shopify_attributes }}>
              <p>{{ block.settings.text | newline_to_br }}</p>
            </div>
          {%- when 'buttons' -%}
            <div
              class="banner__buttons{% if block.settings.button_label_1 != blank and block.settings.button_label_2 != blank %} banner__buttons--multiple{% endif %}"
              {{ block.shopify_attributes }}
            >
              {%- if block.settings.button_label_1 != blank -%}
                <a
                  {% if block.settings.button_link_1 == blank %}
                    role="link" aria-disabled="true"
                  {% else %}
                    href="{{ block.settings.button_link_1 }}"
                  {% endif %}
                  class="button{% if block.settings.button_style_secondary_1 %} button--secondary{% else %} button--primary{% endif %}"
                >
                  {{- block.settings.button_label_1 | escape -}}
                </a>
              {%- endif -%}
              {%- if block.settings.button_label_2 != blank -%}
                <a
                  {% if block.settings.button_link_2 == blank %}
                    role="link" aria-disabled="true"
                  {% else %}
                    href="{{ block.settings.button_link_2 }}"
                  {% endif %}
                  class="button{% if block.settings.button_style_secondary_2 %} button--secondary{% else %} button--primary{% endif %}"
                >
                  {{- block.settings.button_label_2 | escape -}}
                </a>
              {%- endif -%}
            </div>
        {%- endcase -%}
      {%- endfor -%}
    </div>
  </div>
  
  {%- if section.settings.promo_card_image != blank -%}
    <a 
      {% if section.settings.promo_card_link != blank %}
        href="{{ section.settings.promo_card_link }}"
      {% else %}
        role="link" aria-disabled="true"
      {% endif %}
      class="banner__promo-card"
    >
      <div class="banner__promo-card-image">
        {{
          section.settings.promo_card_image
          | image_url: width: 220
          | image_tag:
            loading: 'lazy',
            widths: '110, 220'
        }}
      </div>
      {%- if section.settings.promo_card_title != blank or section.settings.promo_card_subtitle != blank -%}
        <div class="banner__promo-card-content">
          {%- if section.settings.promo_card_title != blank -%}
            <p class="banner__promo-card-title">{{ section.settings.promo_card_title }}</p>
          {%- endif -%}
          {%- if section.settings.promo_card_subtitle != blank -%}
            <p class="banner__promo-card-subtitle">{{ section.settings.promo_card_subtitle }}</p>
          {%- endif -%}
        </div>
      {%- endif -%}
    </a>
  {%- endif -%}
</div>

{%- if section.settings.video != blank -%}
  <script>
    (function() {
      var video = document.getElementById('banner-video-{{ section.id }}');
      if (video) {
        // Force all muting methods
        video.muted = true;
        video.defaultMuted = true;
        video.setAttribute('muted', '');
        video.setAttribute('muted', 'muted');
        video.volume = 0;
        
        // Function to attempt play
        function attemptPlay() {
          var playPromise = video.play();
          if (playPromise !== undefined) {
            playPromise.then(function() {
              // Autoplay started
            }).catch(function(error) {
              console.log('Autoplay prevented:', error);
              // Try intersection observer as fallback
              if ('IntersectionObserver' in window) {
                var observer = new IntersectionObserver(function(entries) {
                  entries.forEach(function(entry) {
                    if (entry.isIntersecting && video.paused) {
                      video.play();
                    }
                  });
                }, { threshold: 0.25 });
                observer.observe(video);
              }
            });
          }
        }
        
        // Multiple attempts with different timings
        attemptPlay();
        
        // Try after DOM fully loaded
        if (document.readyState === 'complete') {
          setTimeout(attemptPlay, 100);
        } else {
          window.addEventListener('load', function() {
            setTimeout(attemptPlay, 100);
          });
        }
        
        // Try when video metadata is loaded
        video.addEventListener('loadedmetadata', attemptPlay);
        
        // Try when video can play through
        video.addEventListener('canplaythrough', attemptPlay);
        
        // Fallback to first user interaction
        var playOnInteraction = function() {
          if (video.paused) {
            video.play();
            document.removeEventListener('touchstart', playOnInteraction);
            document.removeEventListener('click', playOnInteraction);
          }
        };
        document.addEventListener('touchstart', playOnInteraction, { passive: true });
        document.addEventListener('click', playOnInteraction);
        
        // Resume on tab visibility
        document.addEventListener('visibilitychange', function() {
          if (!document.hidden && video.paused) {
            attemptPlay();
          }
        });
      }
    })();
  </script>
{%- endif -%}

{% schema %}
{
  "name": "t:sections.image-banner.name",
  "tag": "section",
  "class": "section",
  "disabled_on": {
    "groups": ["header", "footer"]
  },
  "settings": [
    {
      "type": "image_picker",
      "id": "image",
      "label": "t:sections.image-banner.settings.image.label"
    },
    {
      "type": "image_picker",
      "id": "mobile_image",
      "label": "Mobile image",
      "info": "Optional image optimized for mobile devices. Falls back to main image if not set."
    },
    {
      "type": "video",
      "id": "video",
      "label": "Background video",
      "info": "Video will be used instead of image if provided"
    },
    {
      "type": "range",
      "id": "image_overlay_opacity",
      "min": 0,
      "max": 100,
      "step": 10,
      "unit": "%",
      "label": "t:sections.image-banner.settings.image_overlay_opacity.label",
      "default": 0
    },
    {
      "type": "range",
      "id": "media_blur",
      "min": 0,
      "max": 30,
      "step": 1,
